class Solution(object):
    def convertTemperature(self, celsius):
        """
        :type celsius: float
        :rtype: List[float]
        """
        l = []
        kelv = float(celsius)+273.15
        fah = float(celsius)*(1.80)+32.00
        l.append(float(kelv))
        l.append(float(fah))
        return(l)
